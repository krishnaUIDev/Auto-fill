{
  "manifest_version": 3,
  "name": "Autofill Extension By Krishna",
  "version": "1.0",
  "description": "Autofill custom text using right-click context menu",
  "permissions": ["contextMenus", "activeTab", "storage"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "icons/icon48.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "icons/icon48.png"
  }
}
